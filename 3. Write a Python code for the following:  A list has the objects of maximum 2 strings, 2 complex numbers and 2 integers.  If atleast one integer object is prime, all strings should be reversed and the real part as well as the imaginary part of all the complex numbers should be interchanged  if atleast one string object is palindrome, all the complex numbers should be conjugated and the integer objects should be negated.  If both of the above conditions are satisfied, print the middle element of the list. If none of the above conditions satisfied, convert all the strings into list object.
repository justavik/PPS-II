n = eval(input())
A = []
B = []
C = []
OUT = []
for i in n:
    if type(i) == complex:
        A.append(i)
    if type(i) == int:
        B.append(i)
    if type(i) == str:
        C.append(i)
def prime(l):
    for i in l:
        s = 0
        if i > 1:
            for j in range(1, i):
                if i % j == 0:
                    s += 1
            if s == 1:
                return True
        else:
            return False
def palindrome(p):
    for i in p:
        i = i.lower()
        a = i[::-1]
        if a == i:
            return True
        else:
            return False
x, y, z = 0, 0, 0
for i in n:
    if type(i) == complex:
        x += 1
    if type(i) == int:
        y += 1
    if type(i) == str:
        z += 1
if prime(B) == True:
    for i in n:
        if type(i) == complex:
            temp1 = float(i.real)
            temp2 = float(i.imag)
            a = temp2
            b = temp1
            z = complex(a, b)
            OUT.append(z)
        elif type(i) == int:
            OUT.append(i)
        elif type(i) == str:
            i = i[::-1]
            OUT.append(i)
elif palindrome(C) == True:
    for i in n:
        if type(i) == complex:
            a = i.conjugate()
            OUT.append(a)
        elif type(i) == str:
            OUT.append(i)
        elif type(i) == int:
            b = i*-1
            OUT.append(b)
elif palindrome(C) == True and prime(B) == True:
    print(n[int(len(n)/2)])
else:
    for i in n:
        if type(i) == str:
            for j in i:
                OUT.append(j)
        else:
            OUT.append(i)
print(OUT)
